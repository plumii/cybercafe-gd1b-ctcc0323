    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        WelcomeFrame f2 = new WelcomeFrame();
        f2.show();
        dispose();
    }                                        

    private void radwaterActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        if(radwater.isSelected())
            radcoff.setSelected(false);
            radjuice.setSelected(false);
            
    }                                        

    private void radcoffActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
        if(radcoff.isSelected())
            radwater.setSelected(false);
            radjuice.setSelected(false);
    }                                       

    private void radjuiceActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        if(radjuice.isSelected())
            radcoff.setSelected(false);
            radwater.setSelected(false);
    }                                        

    private void btnEnterActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        double total=0;
        
        if(radwater.isSelected()){
        total=total +15;
        }else if(radcoff.isSelected()){
        total=total +30;
        }else if(radjuice.isSelected()){
        total=total +20.5;
        }
        
        if(cksand.isSelected()){
        total=total +50;
        }if(ckbread.isSelected()){
        total=total +40;
        }if(ckchips.isSelected()){
        total=total +20;
        }if(ckhot.isSelected()){
        total=total +40;
        }if(ckdon.isSelected()){
        total=total +35;
        }if(ckbis.isSelected()){
        total=total +15.5;
        }
        
        totaltxt.setText(Double.toString(total));
        
    }                                        

    private void clrbtnActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        radwater.setSelected(false);
        radcoff.setSelected(false);
        radjuice.setSelected(false);
        cksand.setSelected(false);
        ckbread.setSelected(false);
        ckchips.setSelected(false);
        ckbis.setSelected(false);
        ckhot.setSelected(false);
        ckdon.setSelected(false);
        totaltxt.setText("");
        JOptionPane.showMessageDialog(rootPane, "Items cleared!");
    }                                      

